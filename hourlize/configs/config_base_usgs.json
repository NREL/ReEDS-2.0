{
  "shared": {
    "compression_opts": 4,
    "filetype": "h5",
    "hourly_out_years": [2012],
    "logToTerminal": true,
    "output_timezone": "local",
    "select_year": 2012,
    "start_1am": true
  },

  "resource": {
    "bin_group_cols": ["region","class"],
    "bin_method": "equal_cap_cut",
    "class_bin_method": "kmeans",
    "copy_to_reeds": true,
    "copy_to_shared": true,
    "driver": "H5FD_CORE",
    "dtype": "np.float16",
    "existing_sites": "{reedspath}/inputs/capacitydata/ReEDS_generator_database_final_EIA-NEMS.csv",
    "gather_method": "smart",
    "inputfiles": ["reg_map_file", "class_path"],
    "reg_col": "cnty_fips",
    "reg_map_file": "{hourlizepath}/inputs/resource/county_map.csv",
    "resource_source_timezone": 0, 
    "rev_id_cols": ["sc_point_gid"],
    "rev_paths_file": "{reedspath}/inputs/supplycurvedata/metadata/rev_paths.csv",
    "start_year": 2010,
    "state_abbrev": "{hourlizepath}/inputs/resource/state_abbrev.csv",
    "subsetvars": ["tech", "access_case"],
    "subtract_exog": false,
    "test_filters": {"region":["p97", "p98"]},
    "test_mode": false
  }
}