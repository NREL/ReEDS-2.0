{
  "fileNames": [
    "cap.csv",
    "cap_diff.csv",
    "cap_new_ann.csv",
    "costs.csv",
    "demand.csv",
    "demand_timeslice.csv",
    "firmcap.csv",
    "gen.csv",
    "gen_timeslice.csv",
    "opres.csv",
    "peakdem.csv",
    "style.csv",
    "transmission.csv"
  ],
  "dashboards": [
    {
      "charts": [
        {
          "traces": [
            {
              "dataSource": "cap.csv",
              "type": "bar",
              "agg": "sum",
              "x": "year",
              "name": "tech",
              "col": "scenario",
              "y": "Capacity (MW)"
            }
          ],
          "style": {
            "Plot Width (px)": "1000",
            "Plot Height (px)": "420",
            "Plot Title": "National installed capacity"
          }
        },
        {
          "traces": [
            {
              "dataSource": "cap_new_ann.csv",
              "type": "bar",
              "agg": "sum",
              "x": "year",
              "name": "tech",
              "col": "scenario",
              "y": "Investments (MW)"
            }
          ],
          "style": {
            "Plot Width (px)": "1000",
            "Plot Height (px)": "420",
            "Plot Title": "Annual capacity additions"
          }
        },
        {
          "traces": [
            {
              "dataSource": "firmcap.csv",
              "type": "bar",
              "agg": "sum",
              "x": "year",
              "name": "tech",
              "col": "season",
              "row": "scenario",
              "y": "Firm Capacity (MW)"
            },
            {
              "dataSource": "peakdem.csv",
              "type": "dot",
              "agg": "sum",
              "x": "year",
              "row": "scenario",
              "col": "season",
              "name": "type",
              "y": "PRM requirement (MW)"
            }
          ],
          "style": {
            "Plot Width (px)": "1000",
            "Plot Height (px)": "800",
            "Plot Title": "Firm capacity and PRM requirement"
          }
        },
        {
          "traces": [
            {
              "dataSource": "costs.csv",
              "type": "bar",
              "agg": "sum",
              "x": "scenario",
              "y": "Cost",
              "name": "cost_cat",
              "filter": {
                "Year": {
                  "filter_type": "less-or-equal",
                  "filter_val": "2050"
                }
              }
            }
          ],
          "style": {
            "Y Title": "Cost (INR)",
            "Plot Width (px)": "1000",
            "Plot Title": "Total costs"
          }
        }
      ],
      "title": "Annual capacity results",
      "description": ""
    },
    {
      "charts": [
        {
          "traces": [
            {
              "dataSource": "gen.csv",
              "type": "bar",
              "agg": "sum",
              "x": "year",
              "name": "tech",
              "col": "scenario",
              "y": "Generation (MWh)"
            },
            {
              "dataSource": "demand.csv",
              "type": "dot",
              "agg": "sum",
              "x": "year",
              "name": "type",
              "col": "scenario",
              "y": "Demand (MWh)"
            }
          ],
          "style": {
            "Plot Width (px)": "1000",
            "Plot Height (px)": "400",
            "Plot Title": "National generation and demand"
          }
        },
        {
          "traces": [
            {
              "dataSource": "opres.csv",
              "type": "bar",
              "agg": "sum",
              "x": "year",
              "name": "tech",
              "col": "scenario",
              "y": "Operating Reserves (MWh)"
            }
          ],
          "style": {
            "Plot Width (px)": "1000",
            "Plot Height (px)": "400",
            "Plot Title": "Operating reserves provision"
          }
        }
      ],
      "title": "Annual generation results",
      "description": ""
    },
    {
      "charts": [
        {
          "traces": [
            {
              "dataSource": "gen_timeslice.csv",
              "type": "bar",
              "agg": "sum",
              "x": "time",
              "row": "scenario",
              "col": "season",
              "explode": "year",
              "name": "tech",
              "y": "Generation (MW)"
            },
            {
              "dataSource": "demand_timeslice.csv",
              "type": "dot",
              "agg": "sum",
              "x": "time",
              "name": "type",
              "row": "scenario",
              "col": "season",
              "explode": "year",
              "y": "Demand (MW)"
            }
          ],
          "style": {
            "Plot Width (px)": "1000",
            "Plot Height (px)": "800",
            "Plot Title": "National timeslice generation and demand"
          }
        }
      ],
      "title": "Timeslice generation",
      "description": ""
    }
  ],
  "globalStyleFile": "style.csv",
  "filter": {}
}