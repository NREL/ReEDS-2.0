@article{irizarry-riveraCaseStudyResidential2018,
  title = {A {{Case Study}} of {{Residential Electric Service Resiliency Through Renewable Energy Following Hurricane Maria}}},
  author = {{Irizarry-Rivera}, A. and {Montano-Martinez}, K. and {Alzate-Drada}, S. I. and Andrade, F.},
  year = {2018},
  month = jan,
  pages = {74 (6 pp.)-74 (6 pp.)},
  doi = {10.1049/cp.2018.1906},
  url = {https://digital-library.theiet.org/content/conferences/10.1049/cp.2018.1906},
  urldate = {2019-11-07},
  abstract = {Electrified societies require continuity of electric service to supply basic human needs. Hurricanes in the Caribbean present a significant challenge to the continuity of the electric service. In this paper we present a case study of residential system resiliency with the adaptation of an existing residential photovoltaic system in Puerto Rico, grid-tied under a net metering agreement with the utility, to a stand-alone system with batteries to provide continuity of service after Hurricane Mar{\'i}a. Then, we present and analyze recorded data of the electric demand behavior and essential loads behavior at the residence prior to Hurricane Mar{\'i}a, when the residence was connected to the functioning electric grid at full load, and after Hurricane Maria when the residence was operating disconnected from the electric grid with diminished electric demand. Furthermore, we perform an economic analysis between the PV system and an equivalent gasoline inverter generator. Our analysis serves as a feasibility study of the use of stand-alone photovoltaic systems to increase residential resiliency in Caribbean islands after a hurricane.},
  langid = {english}
}

