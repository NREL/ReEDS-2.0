@inproceedings{jenkinsDynamicModelCombined2015,
  title = {A Dynamic Model of the Combined Electricity and Natural Gas Markets},
  booktitle = {Innovative {{Smart Grid Technologies Conference}} ({{ISGT}}), 2015 {{IEEE Power Energy Society}}},
