@article{ericsonPowerCouplesSynergy2018,
  title = {Power Couples: {{The}} Synergy Value of Battery-Generator Hybrids},
  shorttitle = {Power Couples},
  author = {Ericson, Sean and Anderson, Kate and {Engel-Cox}, Jill and Jayaswal, Harshit and Arent, Doug},
