@article{mignoneCosteffectivenessEconomicIncidence2012,
  title = {Cost-Effectiveness and {{Economic Incidence}} of a {{Clean Energy Standard}}},
  author = {Mignone, Bryan K. and Alfstad, Thomas and Bergman, Aaron and Dubin, Kenneth and Duke, Richard and Friley, Paul and Martinez, Andrew and Mowers, Matthew and Palmer, Karen and Paul, Anthony and Showalter, Sharon and Steinberg, Daniel and Woerman, Matt and Wood, Frances},
