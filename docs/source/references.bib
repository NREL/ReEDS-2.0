@article{leadbetterBatteryStorageSystem2012,
  title = {Battery Storage System for Residential Electricity Peak Demand Shaving},
  author = {Leadbetter, Jason and Swan, Lukas},
  year = {2012},
  month = dec,
  journal = {Energy and Buildings},
  volume = {55},
  pages = {685--692},
  issn = {0378-7788},
  doi = {10.1016/j.enbuild.2012.09.035},
  url = {http://www.sciencedirect.com/science/article/pii/S0378778812004896},
  urldate = {2016-05-02},
  abstract = {This article presents the modeling, simulation, and sizing results of battery energy storage systems for residential electricity peak shaving. Realistic 5 min time-step electricity profiles were input to an energy storage model with the objective of reducing the peak electricity demand seen by the electricity grid. The model simulates and provides performance results of a range of battery and inverter sizes specific to a variety of residential houses. The results suggest typical system sizes range from 5 kWh/2.6 kW for low electricity intensity homes to 22 kWh/5.2 kW for electricity intense homes with electric space heating. Based on the resulting storage system cycling profile and data provided by other researchers, the battery life is estimated to be in the range of 10--20 years for this application.}
}

@article{leeInteractionsComplementaritiesTensions2012,
  title = {Interactions, {{Complementarities}} and {{Tensions}} at the {{Nexus}} of {{Natural Gas}} and {{Renewable Energy}}},
  author = {Lee, April and Zinaman, Owen and Logan, Jeffrey and Bazilian, Morgan and Arent, Douglas and Newmark, Robin L.},
  year = {2012},
  month = dec,
  journal = {The Electricity Journal},
  volume = {25},
  number = {10},
  pages = {38--48},
  issn = {1040-6190},
  doi = {10.1016/j.tej.2012.10.021},
  url = {http://www.sciencedirect.com/science/article/pii/S1040619012002709},
  urldate = {2014-07-24},
  abstract = {Natural gas and renewable energy technologies enjoy many complementarities spanning economic, technical, environmental, and political considerations. These complementarities arise from their similarities---which include improved environmental performance compared to coal and oil and their ability to contribute to a robust U.S. economy---but it is from their dissimilarities that the biggest opportunities for mutually beneficial collaboration can be found.}
