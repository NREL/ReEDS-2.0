  author = {Pelegov, Dmitry V. and Pontes, Jos{\'e}},
  year = {2018},
  month = dec,
  journal = {Batteries},
  volume = {4},
  number = {4},
  pages = {65},
  doi = {10.3390/batteries4040065},
