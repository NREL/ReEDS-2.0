@article{sepulvedaRoleFirmLowCarbon2018a,
  title = {The {{Role}} of {{Firm Low-Carbon Electricity Resources}} in {{Deep Decarbonization}} of {{Power Generation}}},
  author = {{Nestor A. Sepulveda} and {Jesse D. Jenkins} and {Fernando J. de Sisternes} and {Richard K. Lester}},
  year = {2018},
  journal = {Joule},
  volume = {2},
  number = {11},
  pages = {2403--2420},
  publisher = {Elsevier Inc.},
  issn = {25424351},
  doi = {10.1016/j.joule.2018.08.006},
  abstract = {Full decarbonization of the electricity sector is critical to global climate mitigation. Across a wide range of sensitivities, firm low-carbon resources-including nuclear power, bioenergy, and natural gas plants that capture CO 2-consistently lower the cost of decarbonizing electricity generation. Without these resources, costs rise rapidly as CO 2 limits approach zero. Batteries and demand flexibility do not obviate the value of firm resources. Improving the capabilities and spurring adoption of firm low-carbon technologies are key research and policy goals.}
}

@article{serdiofernandezDetectingClippingPhotovoltaic2016,
  title = {Detecting Clipping in Photovoltaic Solar Plants Using Fuzzy Systems on the Feature Space},
