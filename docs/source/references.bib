@misc{gerowLegalPathwaysDeep2021,
  type = {{{SSRN Scholarly Paper}}},
  title = {Legal {{Pathways}} to {{Deep Decarbonization}} in the {{Fields}} of {{Land Use}} and {{Zoning}}},
