@inproceedings{braamPeakShavingPhotovoltaicbattery2014,
  title = {Peak Shaving with Photovoltaic-Battery Systems},
  booktitle = {{{IEEE PES Innovative Smart Grid Technologies}}, {{Europe}}},
  author = {Braam, F. and Hollinger, R. and Engesser, M. L. and M{\"u}ller, S. and Kohrs, R. and Wittwer, C.},
  year = {2014},
  month = oct,
  pages = {1--5},
  doi = {10.1109/ISGTEurope.2014.7028748},
  abstract = {With the steady growth of the photovoltaic market over the last decade, the feed-in of photovoltaic-generators may strain the electricity grid, especially in rural areas. Storage devices are expected to smooth over the non-simultaneous production of volatile renewable energies and their consumption. In this paper, we investigate to what extent domestic photovoltaic-battery systems can compensate the capping of the feed-in power by buffering the peak energy. The Fraunhofer Institute for Solar Energy Systems (ISE) in Freiburg, Germany, has developed a novel forecast-based control scheme for photovoltaic-battery systems. It estimates the photovoltaic excess energy and derives a charging schedule for the battery to store the energy from the photovoltaic peak-production. The maximal self-consumption of the locally produced renewable energy is thereby not affected. We show that the maximal photovoltaic feed-in power can be reduced from 70 \% to 60 \% of the nominal photovoltaic power without losses.}
}

