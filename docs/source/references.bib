@article{sunClimateImpactsHydrogen2024,
  title = {Climate {{Impacts}} of {{Hydrogen}} and {{Methane Emissions Can Considerably Reduce}} the {{Climate Benefits}} across {{Key Hydrogen Use Cases}} and {{Time Scales}}},
  author = {Sun, Tianyi and Shrestha, Eriko and Hamburg, Steven P. and Kupers, Roland and Ocko, Ilissa B.},
  year = {2024},
  month = mar,
  journal = {Environmental Science \& Technology},
  volume = {58},
  number = {12},
  pages = {5299--5309},
  issn = {0013-936X, 1520-5851},
  doi = {10.1021/acs.est.3c09030},
  url = {https://pubs.acs.org/doi/10.1021/acs.est.3c09030},
  urldate = {2024-06-01},
  abstract = {Recent investments in ``clean'' hydrogen as an alternative to fossil fuels are driven by anticipated climate benefits. However, most climate benefit calculations do not adequately account for all climate warming emissions and impacts over time. This study reanalyzes a previously published life cycle assessment as an illustrative example to show how the climate impacts of hydrogen deployment can be far greater than expected when including the warming effects of hydrogen emissions, observed methane emission intensities, and near-term time scales; this reduces the perceived climate benefits upon replacement of fossil fuel technologies. For example, for blue (natural gas with carbon capture) hydrogen pathways, the inclusion of upper-end hydrogen and methane emissions can yield an increase in warming in the near term by up to 50\%, whereas lower-end emissions decrease warming impacts by at least 70\%. For green (renewable-based electrolysis) hydrogen pathways, upper-end hydrogen emissions can reduce climate benefits in the near term by up to 25\%. We also consider renewable electricity availability for green hydrogen and show that if it is not additional to what is needed to decarbonize the electric grid, there may be more warming than that seen with fossil fuel alternatives over all time scales. Assessments of hydrogen's climate impacts should include the aforementioned factors if hydrogen is to be an effective decarbonization tool.},
  copyright = {https://creativecommons.org/licenses/by-nc-nd/4.0/},
  langid = {english}
}

