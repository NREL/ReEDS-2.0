@article{wrightLowCarbonStandard2014,
  title = {Low Carbon Standard and Transmission Investment Analysis in the New Multi-Region {{US}} Power Sector Model {{FACETS}}},
  author = {Wright, Evelyn and Kanudia, Amit},
  year = {2014},
  month = nov,
  journal = {Energy Economics},
  volume = {46},
  pages = {136--150},
  issn = {0140-9883},
  doi = {10.1016/j.eneco.2014.09.013},
  url = {http://www.sciencedirect.com/science/article/pii/S0140988314002230},
  urldate = {2015-09-14},
  abstract = {This paper presents a new US multi-region energy systems model built in the TIMES modeling system: the Framework for Analysis of Climate-Energy-Technology Systems (FACETS). The model is designed to analyze energy technology options and policy scenarios across sectors and regions, including the increasingly important interactions between state, regional, and federal policies. FACETS contains a realistic representation of key infrastructure, while retaining the flexibility to explore deep carbon emission reductions and other large changes from the baseline energy system. It is built using a unique, flexible multi-region approach so that the geographic relationships that drive the costs of energy technology transitions can be captured. Significant enhancements to the Veda-TIMES system and a GIS-results viewer permit the massive data handling needed to represent these relationships and interpret results. An analysis of a federal clean energy standard (CES) and investment in the transmission grid is presented.}
}

