@article{sitholeDevelopingOptimalElectricity2016,
  title = {Developing an Optimal Electricity Generation Mix for the {{UK}} 2050 Future},
  author = {Sithole, H. and Cockerill, T. T. and Hughes, K. J. and Ingham, D. B. and Ma, L. and Porter, R. T. J. and Pourkashanian, M.},
  year = {2016},
  month = apr,
  journal = {Energy},
